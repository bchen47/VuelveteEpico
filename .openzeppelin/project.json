{
  "manifestVersion": "2.2",
  "contracts": {
    "MyContract": "MyContract",
    "Migrations": "Migrations"
  },
  "dependencies": {
    "@openzeppelin/contracts-ethereum-package": "^3.0.0"
  },
  "name": "counter-app",
  "version": "0.1.0",
  "compiler": {
    "compilerSettings": {
      "optimizer": {
        "enabled": false,
        "runs": "200"
